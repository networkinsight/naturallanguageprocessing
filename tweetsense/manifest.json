{

  "manifest_version": 2,
  "name": "TweetSense",
  "version": "1.0",

  "description": "Sentinent analysis and color highlighting of Twitter content.",

  "icons": {
    "48": "res/logo-48.png",
    "96": "res/logo-96.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.twitter.com/*"],
      "js": ["tweetsense.js"]
    }
  ]

}
